{
  "name": "schoolable",
  "version": "0.0.1",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^6.9.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.0-next.12",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.12",
    "@types/js-yaml": "^4.0.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^15.12.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.0",
    "js-yaml": "^3.14.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "config": {
    "mongodbMemoryServer": {
      "downloadDir": "/path/to/mongodb/binaries",
      "platform": "linux",
      "arch": "x64",
      "version": "3",
      "debug": "0",
      "downloadMirror": "url",
      "disablePostinstall": "1",
      "systemBinary": "/path/to/mongodb/binaries/4.2.8/mongod",
      "md5Check": "1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hardekerlis/schoolable.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hardekerlis/schoolable/issues"
  },
  "homepage": "https://github.com/Hardekerlis/schoolable#readme",
  "description": ""
}
