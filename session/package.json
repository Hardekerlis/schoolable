{
  "name": "session",
  "version": "1.0.0",
  "description": "Session handling service for the Schoolable platform",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
