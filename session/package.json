    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.13.0",
    "geoip-lite": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "node-nats-streaming": "^0.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
